">\r\n            \t\t<div class=\"hero-anchor\">\r\n        \t\t\t\t<h6>InDesign</h6>\r\n        \t\t\t\t<p><a href=\"http://vector.tutsplus.com/tutorials/tools-tips/illustrators-pen-tool-the-comprehensive-guide/\" target=\"_blank\">Pen Tool Guide</a></p>\r\n        \t\t\t\t<p><a href=\"http://www.instantindesign.com/\" target=\"_blank\">Instant Indesign</a></p>\r\n            \t\t\t<p><a href=\"http://vector.tutsplus.com/\" target=\"_blank\">Tuts+ Vector</a></p>\r\n        \t\t\t\t<p><a href=\"http://vectips.com/\" target=\"_blank\">Vec Tips</a></p>\r\n        \t\t\t</div>\r\n                </div>\r\n            </div>\r\n\t\t</div>\r\n\t\t<!-- END resources -->\r\n\t</div>\r\n\t<!-- END resources and training -->\r\n</section>\r\n<!-- END tools -->\r\n\r\n<script>\r\n                    \r\n    $( document ).ready( function () {\r\n        \r\n        // if globals valid\r\n        \r\n        if ( CKH && CKH.classes ) {\r\n            \r\n            CKH.classes.AddClass( {\r\n                name: 'Typography: Basics',\r\n                summary: 'An introduction to the <span class=\"logo-type-purple\">core</span> <span class=\"logo-type-blue\">of</span> <span class=\"logo-type-green\">communication</span>.',\r\n                navigation: {\r\n                    main: [\r\n                        '<a href=\"#classIntroduction\">Introduction</a>',\r\n                        '<a href=\"#classTools\">Tools</a>'\r\n                    ]\r\n                },\r\n                calendar: {\r\n                    embed: 'https://www.google.com/calendar/embed?mode=AGENDA&showTitle=0&showPrint=0&showCalendars=0&height=600&wkst=1&bgcolor=%23FFFFFF&src=2qilak7vl5uga3m7kiu3sf2g5g%40group.calendar.google.com&color=%235229A3&ctz=America%2FChicago',\r\n                    link: 'https://www.google.com/calendar/htmlembed?mode=AGENDA&src=2qilak7vl5uga3m7kiu3sf2g5g%40group.calendar.google.com'\r\n                }\r\n            } );\r\n        \r\n        }\r\n    \r\n    }